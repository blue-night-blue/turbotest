<h1>Quotes</h1>
  
<!-- ■■■■■■■■■■turbo該当箇所■■■■■■■■■■ -->
<div style="margin-bottom: 4em;">
  <%= turbo_frame_tag "new_quote" do %>
      <%= render "form", quote: @quote %>
  <% end %>
</div>

<!-- ■■■■■■■■■■turbo該当箇所■■■■■■■■■■ -->
<%= turbo_frame_tag "quotes" do %>
  <%= render @quotes %>
<% end %>
